{
  "name": "@smartlyio/oats-mirage-adapter",
  "version": "0.0.3",
  "description": "Mirage.js adapter for Oats",
  "licence": "MIT",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf ./dist && rm -f ./tmp/*.ts",
    "build": "yarn clean && yarn lint && yarn render && yarn tsc",
    "prepublish": "yarn build",
    "render": "ts-node render-docs.ts",
    "watch": "nodemon --ignore dist/ -e .ts,.json -x yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats-mirage-adapter.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.12.1",
    "miragejs": "^0.1.41",
    "typescript": "^4.0.0"
  },
  "keywords": [
    "oats",
    "mirage",
    "mirage.js",
    "pretender",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server"
  ],
  "devDependencies": {
    "@smartlyio/oats-runtime": "2.12.1",
    "@types/lodash": "4.14.168",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.24.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "3.3.1",
    "miragejs": "0.1.41",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}

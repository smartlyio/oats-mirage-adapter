{
  "name": "@smartlyio/oats-mirage-adapter",
  "version": "0.0.1",
  "description": "Mirage.js adapter for Oats",
  "private": true,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf ./dist && rm -f ./tmp/*.ts",
    "build": "yarn clean && yarn lint && yarn tsc",
    "prepublish": "yarn build",
    "render": "ts-node render-docs.ts",
    "render:watch": "nodemon -w \"examples/\" -w \"README.template.md\" render-docs.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.8.0",
    "koa": "^2.3.0",
    "typescript": "^4.0.0"
  },
  "keywords": [
    "oats",
    "koa",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-runtime": "^2.12.1",
    "@types/lodash": "4.14.165",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.15.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "3.3.0",
    "miragejs": "^0.1.41",
    "nodemon": "2.0.6",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "^4.2.3"
  }
}
